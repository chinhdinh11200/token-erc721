    "deploy_beki_nft": "hardhat run --network bscTestnet scripts/deploy-BekiNFT.ts",
    "deploy": "hardhat run --network marketplace scripts/deploy.ts",
    "account": "hardhat run --network marketplace scripts/account.ts"
  },
